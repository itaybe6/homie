{
  "expo": {
    "name": "Homie",
    "slug": "homie",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "homie",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.homie.sharedliving",
      "supportsTablet": true,
      "buildNumber": "1",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "אנחנו משתמשים במיקום שלך כדי להציג דירות ושותפים קרובים אליך.",
        "NSCameraUsageDescription": "נשתמש במצלמה כדי להעלות תמונות לדירה.",
        "NSPhotoLibraryUsageDescription": "נשתמש בגלריה כדי לבחור תמונות לדירה.",
        "NSPhotoLibraryAddUsageDescription": "נוכל לשמור תמונות לדירה בגלריה לפי הצורך.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.homie.sharedliving",
      "versionCode": 1,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-notifications",
      "@react-native-community/datetimepicker"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "updates": {
      "url": "https://u.expo.dev/6b12436b-5a01-4115-a54c-da9cb6a756e7"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6b12436b-5a01-4115-a54c-da9cb6a756e7"
      }
    }
  }
}
